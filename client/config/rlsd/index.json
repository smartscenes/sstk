{
    "scene_manager":{
        "save_endpoint":{
            "url_prefix": "${rlsdBaseUrl}/scene-manager/editor/"
        },
        "backend_url_prefix": "${rlsdBaseUrl}/api/scene-manager/",
        "scene_json_endpoint": {
            "url": "scenes/sceneMangerId/json"
        }
    },
    "scene_wizard": {
        "pose_suggestor_endpoint": {
            "url": "${rlsdBaseUrl}/api/wizard/pose_suggester"
        },
        "shape_suggestor_endpoint": {
            "url": "${rlsdBaseUrl}/api/wizard/shape_suggester"
        },
        "object_similarity_endpoint": {
            "url": "${rlsdBaseUrl}/api/wizard/objects/similar"
        }
    },
    "object_info_store":{
        "label_field": "rlsd48Name",
        "allowed_object_labels": [
            "stove", "washing machine", "refrigerator", "oven", "microwave", "dishwasher", "clothes dryer", 
            "chair", "seating", "sofa", "stool", "cabinet", "chest_of_drawers", "shelving", 
            "bed", "tv_monitor", "table", "counter", "cushion", "lighting", "picture", "plant", "blinds", 
            "curtain", "mirror", "toilet", "bathtub", "shower", "sink", "towel"
        ]
    },
    "putOnArchOnly": {
        "disable_wnlemmas": ["pillow", "cushion", "table lamp"],
        "disable_mask_labels": ["pillow", "cushion", "lighting"]
    },
    "attachment_surface": {
        "bottom": ["stove", "washing machine", "refrigerator", "oven", "microwave", "dishwasher", "clothes dryer", "chair",
        "sofa", "stool", "cabinet", "chest_of_drawers", "bed", "table", "counter", "cushion", "plant", "toilet", "bathtub", "shower", "sink"],
        "back": ["shelving", "tv_monitor", "lighting", "picture", "curtain", "mirror", "clothes"]
    },
